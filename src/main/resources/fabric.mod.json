{
  "schemaVersion": 1,
  "id": "spookiar",
  "version": "${version}",

  "name": "Spookiar",
  "description":"A mod that adds a spectacularly spooky experience in Minecraft!",
  "authors": [
    "LunarLuned",
    "ElBones: Artist, extra texture work",
    "Ambersand: Artist, Major contributions to audio and textures",
    "Thaetaa-Terraainn: Music Composer, Creator of 'JUSTICE'"
  ],
  "contact": {
    "homepage": "https://www.twitter.com/CosmaMods",
    "sources": "https://github.com/LunarLuned/Peculia"
  },
  "license": "MIT",
  "icon": "assets/spookiar/icon.png",
  "accessWidener": "spookiar.accesswidener",
